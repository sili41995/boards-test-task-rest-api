generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model User {
    id       Int     @id @default(autoincrement())
    name     String
    email    String  @unique
    password String
    token    String?
    // boards   Boards[]
}

model Board {
    id Int @id @default(autoincrement())
}

model Task {
    id Int @id @default(autoincrement())
}
